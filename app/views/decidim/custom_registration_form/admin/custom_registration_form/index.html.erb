<div class="card" id="">
  <div class="card-divider">
    <h2 class="card-title">
      <%= t "decidim.admin.titles.custom_registration_form" %>
    </h2>
  </div>
  <div class="card-section">
    <p>Content in progress</p>

    <%= decidim_form_for(@form, html: { class: "form custom_registration_form", id: "custom_registration_form" }, url: "/admin") do |f| %>
      <div class="card">
        <div class="card-divider">
          <h2 class="card-title">Mon titre</h2>
        </div>
        <div class="card-section">
          <%= render partial: "form", object: f %>
        </div>
      </div>
      <div class="button--double form-general-submit">
        <%= f.submit "Save" %>
      </div>
    <% end %>
  </div>
</div>
